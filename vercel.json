{
  "version": 2,
  "builds": [
    {
      "src": "App/app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/templates/(.*)",
      "dest": "/App/templates/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/App/app.py"
    }
  ],
  "env": {
    "FLASK_ENV": "production",
    "FLASK_APP": "App/app.py"
  },
  "functions": {
    "api": {
      "memory": 1024
    }
  }
}
